.custom-tabbar {
  position: fixed;
  bottom: 0;
}

.container {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.basics {
  flex: 1;
  background-color: lavenderblush;
  padding-bottom: calc(100rpx + env(safe-area-inset-bottom) / 2);
}

.search-view
{
  position: relative;
  background-color: white;
  padding: 16rpx 20rpx;
}

.search-cover
{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  background-color:rgba(1, 1, 1, 0);
  z-index: 99;
}



